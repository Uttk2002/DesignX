  xhrPostCall(url, analyticObj);

};

function xhrPostCall(url, body) {
    return new Promise(function (resolve, reject) {
      var xhr = new XMLHttpRequest();
      xhr.open('POST', url);
//       xhr.responseType = 'json';
      xhr.setRequestHeader("Content-Type", "application/json");
      xhr.onload = function () {
